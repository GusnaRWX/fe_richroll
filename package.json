{
  "name": "kayaroll-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:json",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix && npm run lint:json:fix",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint **/*.{scss,css}",
    "lint:css:fix": "stylelint --fix **/*.{scss,css}",
    "lint:json": "prettier --check \"./**/*.json\"",
    "lint:json:fix": "prettier --write \"./**/*.json\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "antd": "^5.2.3",
    "clsx": "^1.2.1",
    "next": "13.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.61.0"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "stylelint": "^15.4.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-scss": "^4.6.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --verbose -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  }
}
